{
  "name": "snapdragon-location",
  "description": "Adds a location object to snapdragon token or AST node.",
  "version": "1.0.0",
  "homepage": "https://github.com/here-be/snapdragon-location",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "here-be/snapdragon-location",
  "bugs": {
    "url": "https://github.com/here-be/snapdragon-location/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "nyc mocha"
  },
  "devDependencies": {
    "@sellside/emitter": "^1.2.1",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3",
    "nyc": "^11.4.1",
    "snapdragon-lexer": "^1.0.0",
    "use": "^3.1.0"
  },
  "keywords": [
    "compile",
    "compiler",
    "convert",
    "lexer",
    "location",
    "parse",
    "parser",
    "position",
    "range",
    "render",
    "snapdragon",
    "transform"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "snapdragon-capture",
        "snapdragon-node",
        "snapdragon-util"
      ]
    },
    "reflinks": [
      "snapdragon-lexer",
      "snapdragon-position",
      "snapdragon-tokenizer"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
